snippet con "console log"
console.log($1)
endsnippet

snippet cv "console var"
console.log('$1:', $1)
endsnippet

snippet fun "anonymous function"
function() {
	$1
}
endsnippet

snippet funwrap "wrap with function"
function() {
	${VISUAL}
}
endsnippet

snippet it "test"
it('$1', () => {
	$2
})
endsnippet
